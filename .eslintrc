{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"rules": {
		"semi": 2,
		"eqeqeq": "off",
		"curly": "error",
		"quotes": ["error", "double"],
		"no-unused-labels": "warn",
		"no-mixed-spaces-and-tabs": "off",
		"react/prop-types": "off"
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	}
}
